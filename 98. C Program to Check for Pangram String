#include <stdio.h>
#include <ctype.h>

int main() {
    char str[200];
    int alpha[26] = {0};
    int i;

    printf("Enter a string: ");
    gets(str);   // for exam purpose

    for (i = 0; str[i] != '\0'; i++) {
        if (isalpha(str[i])) {
            int index = tolower(str[i]) - 'a';
            alpha[index] = 1;
        }
    }

    for (i = 0; i < 26; i++) {
        if (alpha[i] == 0) {
            printf("Not a Pangram\n");
            return 0;
        }
    }

    printf("Pangram String\n");
    return 0;
}

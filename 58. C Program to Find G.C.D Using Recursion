#include <stdio.h>

// Function to find GCD using recursion
int gcd(int a, int b) {
    if (b == 0)          // Base case: if second number is 0
        return a;
    else
        return gcd(b, a % b);  // Recursive call
}

int main() {
    int num1, num2;

    printf("Enter two positive integers: ");
    scanf("%d %d", &num1, &num2);

    printf("G.C.D of %d and %d is %d\n", num1, num2, gcd(num1, num2));

    return 0;
}
